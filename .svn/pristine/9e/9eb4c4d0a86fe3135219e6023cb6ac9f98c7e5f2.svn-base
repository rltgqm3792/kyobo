.icon_sam {position:absolute;left:10px;top:10px; width:60px; height:26px; display:inline-block;background:url('../images/icon_sam.png') no-repeat 0 0; text-indent:-9999px;z-index:11;background-size:60px 26px;}
.icon_free {position:absolute;left:10px;top:10px; width:60px; height:26px; display:inline-block;background:url('../images/icon_free.png') no-repeat 0 0; text-indent:-9999px;z-index:11;background-size:60px 26px;}
.icon_audio {position:absolute;right:0px;bottom:0px; width:60px;height:60px;background: url('../images/icon_audio.png') no-repeat 10px 10px;text-indent:-9999px;z-index:11;background-size:40px 40px;}
.icon_small01 {display: inline-block; min-width:124px; height:28px; line-height:26px; padding:0 10px; font-size:14px; border-radius: 14px; margin-left:1px; vertical-align:middle;text-decoration:none; text-align: center; border:1px solid #666; font-family: 'NotoM';}  
.icon_small01 .icon_text_sam {display:inline-block;font-size:0; width:36px; height:24px; line-height:0; text-indent: -999em; background:url('../images/icon_sam_small.png') no-repeat 0 5px; background-size:36px 11px; vertical-align: text-top; margin-right:6px;}  

/* body fixed  */
.body_fixed {position:fixed; bottom:0; width:100%; height:53px; padding:0px 0 0 0; margin-left:-36px; background:#fff; border-top:1px solid #cccccc; z-index:1000;}
.body_fixed > .bottom_box {position: relative; width:100%; z-index:1100;}

.pagecount {position: absolute; left:36px; top:-2px; font-size:16px;  font-family: 'NotoM'; padding-top:15px;}
.pagecount em {padding:0 5px;}
.pagecount em:nth-of-type(1) {font-family: 'NotoBk'; color:#000;}
/* pagination */
.pagination {position: absolute; right:15px; top:5px;}
.pagination:after {display:block;clear:both;content:''}
.pagination .num > strong {color:#fff; padding:2px 8px 2px 8px; text-align: center; background: #333; border-radius: 18px;}
.pagination a:link,
.pagination a:active,
.pagination a:visited {text-decoration:none;}
.pagination a:hover, .pagination .num > a:hover {text-decoration:none; border:0px solid #fff;}
.pagination .num {display:inline-block; padding:4px 0px 0 0px; margin-top:0px;}
.pagination .num > a , .pagination .num > strong {display:inline-block; min-width:30px; height:30px; line-height:27px; font-family:'NotoB'; color:#000; font-size:16px; text-align:center; margin:0 18px 0 0; border:0px solid #dedede; box-sizing: border-box;}
.pagination .num > strong {color:#fff; font-family:'NotoM';}
.pagination .num > a:first-child , .pagination .num > strong:first-child {background:none;}
.pagination > a {display:inline-block; font-size:0px;line-height:0; text-indent: -9999px; width:34px;height:30px; vertical-align:middle;}
.pagination a.btn_first {background:url('../images/icon_pprev02.png') no-repeat 50% 50%; background-size:12px 12px; margin:-5px 0 0 10px;}
.pagination a.btn_prev {background:url('../images/icon_pprev01.png') no-repeat 50% 50%; background-size:12px 12px; margin:-5px 10px 0 10px;}
.pagination a.btn_next {background:url('../images/icon_pnext01.png') no-repeat 50% 50%; background-size:12px 12px; margin:-5px 0 0 -6px;}
.pagination a.btn_last {background:url('../images/icon_pnext02.png') no-repeat 50% 50%; background-size:12px 12px;margin:-5px 0 0 10px;}

.bottom_box {position: relative; padding:0 28px 0 28px; box-sizing: border-box;}
.bottom_box:after {display:block;clear:both;content:''}
.bottom_box .btn_left {position: absolute; top:-4px; left:19px; width:20%; padding-top:10px;}
.bottom_box .btn_left a.btn_zzim01 {display:block;font-size:0; width:46px; height:46px; line-height:0; text-indent: -999em; background:url('../images/icon_zzim01.png') no-repeat 50% 5px; background-size:36px 36px;}
.bottom_box .btn_right {position: absolute; top:0; right:12px; width:80%; text-align: right;}
.bottom_box .btn_right > ul.btn {}
.bottom_box .btn_right > ul:after {display:block;clear:both;content:''}
.bottom_box .btn_right > ul.btn > li {position: relative; display: inline-block; padding:0 0;}
.bottom_box .btn_right > ul.btn > li:first-child {padding:0;}
.bottom_box .btn_right > ul.btn > li:first-child:after {content:''}
.bottom_box .btn_right > ul.btn > li:after {position: absolute; top:16px; left:0px; font-size:16px; color:#999; font-family: 'NotoL'; display:inline-block; clear:both; 
content:'|'}

/* tab01 */
ul.tab01 {width:100%; margin-top:6px; display:table; border-bottom:1px solid #999999;}
ul.tab01 li {display:inline-block; font-size: 18px; font-family: 'NotoR'; width:33.33%; text-align: center;}
ul.tab01 li a {display:block; padding-top:19px; padding-bottom:8px; color: #000;}
ul.tab01 li a.on {display:inline-block; width:100%; font-family: 'NotoB'; background:url('../images/bg_gnb01.png') repeat-x 0 100%;}
.tab_cont {padding:24px 0; font-size: 20px; font-family: 'NotoL'; line-height: 30px;}

/* skeleton */
/* book_list01 */
.book_list01.skeleton {width:100%; margin:7px auto 0;}
.book_list01.skeleton .list {position: absolute; top:0; left:0;  display:flex; flex-flow:wrap; justify-content: space-between; margin-top:-18px; z-index: 1;}
.book_list01.skeleton .list li > a::before {content:""; display:block; clear:both;}
.book_list01.skeleton .list li {position:relative; flex: 0 0 auto; margin:20px 0 0; width: calc(33.33% - 30px);}
.book_list01.skeleton .list li .pic {position: relative; border:1px solid #e6e6e6; z-index:10;} 
.book_list01.skeleton .list li .pic {width:178px; height:260px; background:#e6e6e6;}
.book_list01.skeleton .list li .title {font-size:18px; color:#000; padding-top:8px;height:35px;}


