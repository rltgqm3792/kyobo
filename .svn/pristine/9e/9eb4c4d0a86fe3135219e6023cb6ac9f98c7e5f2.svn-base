@charset "utf-8";
/** Reset **/
html, body {height:100%;}
/* * {-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;-webkit-text-size-adjust:none;}
*:before,
*:after {-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;} */
body {font-size:16px; line-height:1.4; font-family:'NotoM', ’Malgun Gothic’, Dotum, Arial, Sans-serif; color:#000; letter-spacing: -0.5px;}
img{max-width:100%;} /* 폰트스타일: 사이트 특성에 따라 변경 가능 */
table, th, td, input, select, button, textarea, pre {font:inherit; color:inherit;}
body, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, select, button, textarea, p, blockquote, table, th, td {margin:0; padding:0;}
h1, h2, h3, h4, h5, h6 {font-size:100%; font-weight: normal;}
ol, ul {list-style:none;}
img, fieldset {border:0;}
img {vertical-align:middle; }
a {color:#000; text-decoration:none;}
table {border-spacing:0; border-collapse:collapse; border:0;}
th, td {word-break:break-all; word-wrap:break-word;}
label {cursor:pointer;}
Input, button {border:0; border-radius:0;}
textarea {resize:none;}
input, button, select, textarea {vertical-align:middle; font-family: 'NotoM';}
address, caption, cite, code, dfn, em, var {font-style:normal;}
hr {clear:both; display:none;}
blockquote, q {quotes:none;}
abbr, acronym {border:0;}
legend, caption {visibility:hidden; overflow:hidden; line-height:0; width:0; height:0; margin:0; padding:0; text-indent:-99999px; font-size:0;}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section, main {display:block;}

.hidden {position:absolute; visibility:hidden; overflow:hidden; height:0; line-height:0; width:0; margin:0; padding:0; font:0/0 Arial;}

.scrollNo {overflow:hidden;}
/* form */
input {font-size:15px; border:1px solid #d7d7d7; height:32px; padding:3px 8px 2px 8px; background:#fff; vertical-align:top; border-radius: 0px;  -webkit-appearance: none;  -moz-appearance: none; appearance: none;}

/* Medium */
@font-face {
	font-family: 'NotoM';
	font-style: normal;
	font-weight: 500;
	src: local('Noto Sans Medium'), local('NotoSans-Medium');
	src: /* url(/fonts/NotoSans-Medium.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Medium.woff2) format('woff2'),
	url(../font/NotoSansKR-Medium.woff) format('woff');
	}
/* Bold */
@font-face {
	font-family: 'NotoB';
	font-style: normal;
	font-weight: 700;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Bold.woff2) format('woff2'),
	url(../font/NotoSansKR-Bold.woff) format('woff');
}
/* Regular */
@font-face {
	font-family: 'NotoR';
	font-style: normal;
	font-weight: 300;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Regular.woff2) format('woff2'),
	url(../font/NotoSansKR-Regular.woff) format('woff');
}
/* Black */
@font-face {
	font-family: 'NotoBk';
	font-style: normal;
	font-weight: 700;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Black.woff2) format('woff2'),
	url(../font/NotoSansKR-Black.woff) format('woff');
}

/* Light */
@font-face {
	font-family: 'NotoL';
	font-style: normal;
	font-weight: 300;
	src: local('Noto Sans Light'), local('NotoSans-Light');
	src: /* url(/fonts/NotoSans-Light.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Light.woff2) format('woff2'),
	url(../font/NotoSansKR-Light.woff) format('woff');
}

select { height:32px; font-size:18px; font-family: 'NotoM'; padding:3px 5px 3px 5px; vertical-align:top; margin-right:6px; /*background:#fff url('../images/icon_select01.png') no-repeat right center;*/ background-size:auto 8px;	border:0px; border-radius: 0px;  -webkit-appearance: none;  -moz-appearance: none; appearance: none; outline:none !important; box-sizing: border-box;}
select::-ms-expand { display: none;}
.selectbox.w01 select {width: 168px !important;}
.selectbox.w02 select {width: 268px !important;}

.selectbox.w01 .select {width: 168px !important;}
.selectbox.w02 .select {width: 268px !important;}
.select-hidden {display: none; visibility: hidden; padding-right: 10px;}
.select {cursor: pointer; display: inline-block; position: relative; font-size: 18px; color: #333; letter-spacing:-0.5px; width: 168px; height: 40px;}
.select-styled {position: absolute; top: 0; right: 0; bottom: 0; left: 0; padding: 8px 15px; -moz-transition: all 0.2s ease-in; -o-transition: all 0.2s ease-in; -webkit-transition: all 0.2s ease-in; transition: all 0.2s ease-in;}
.select-styled:after {content: ""; width: 15px; height: 10px; background:url('../images/icon_select.png') no-repeat right center;  background-size: 15px 9px; position: absolute;
	top: 16px; right: 10px;}
.select-styled:hover {background-color: #fff;}
.select-styled:active, .select-styled.active {background-color: #fff;}
.select-styled:active:after, .select-styled.active:after {top: 15px; background:url('../images/icon_select_on.png') no-repeat right center;}
.select-options {display: none; position: absolute; top: 100%; right: 0; left: 0; z-index: 999; margin: 0; padding: 0; list-style: none; background-color: #fff; border: 1px solid #333;}
.select-options li {margin: 0; padding:6px 0 5px; text-indent: 15px; border-top: 0px solid #333; -moz-transition: all 0.15s ease-in; -o-transition: all 0.15s ease-in;
	-webkit-transition: all 0.15s ease-in; transition: all 0.15s ease-in;}
.select-options li:hover {color: #c0392b; background: #fff;}
.select-options li[rel="hide"] {display: none;}   

/** layout **/
.pagemain {overflow: hidden;}
.pagesub {overflow: hidden;}
#m_wrap {position:relative; width:100%;}
#header_container {}
#header_container .header_fixed {position:fixed; top:0; width:100%; height:53px; padding:0; background:#fff; border-bottom:1px solid #999; z-index:1100;}
/* main */
.pagemain #header_container .header_fixed {height:112px !important;}

#body_container {width:100%; margin:53px auto 0px; padding:0 0 100px;  z-index:1000;}
#body_container:after {display:block;clear:both;content:''}

#body_container .content {width:100%; height:100%; padding:16px 0 20px; box-sizing: border-box;}
#body_container .content_inner {width:100%; height:758px; padding:4px 28px; overflow-y: auto; box-sizing: border-box;}
/* pageMain */
.pagemain #body_container {margin:112px auto 0px;}
.pagemain #body_container .content_inner {width:100%; height:706px; padding:0px 36px; overflow:hidden; box-sizing: border-box;}

/* content bottom  */
.content_bottom {position: relative; width:100%; height:60px; padding:0px 0 0 0; margin-left:0px; margin-right:0;  background:#000;z-index:1000;}
.content_bottom > .bottom_box {position: relative; width:100%; z-index:1100;}

/* pageMain */
.pagemain .content_bottom {position: relative; width:100%; height:53px; padding:0px 0 0 0; margin-left:0px; margin-right:0;  background:#fff; border-top:1px solid #cccccc; z-index:1000;}
.pagemain .content_bottom > .bottom_box {position: relative; width:100%; z-index:1100;}

#footer_container {position:fixed; bottom:0; width:100%; height:66px; padding:0px 0 0 0; background:#fcc; z-index:1000;}
#footer_container > .inner {position: absolute; top:0; left:0; margin:0 auto; color:#000; background:#00f; z-index:1100;}

/** content **/
.header_top {position: relative; padding:0 36px;}
	/* btn menu */
	.header_top a.btn_menu {position:absolute; top:10px; left:26px; width:52px; height:58px; display:block;font-size:0; line-height:0; background:url('../images/icon_menu.png') no-repeat 10px 10px; background-size:32px 28px; text-indent: -999em;}
	/* back */
	.header_top .btn_back {position:absolute; top:8px; left:14px; width:36px; height:28px; background:url('../images/icon_back.png') no-repeat 14px 2px; background-size:26px 32px;display:block;font-size:0; width:52px; height:38px; line-height:0; text-indent: -999em;}
	.header_top .top_title {position: absolute; top:16px; left:69px; color:#787878;}
	.header_top .top_title > h1 > span {display: inline-block; padding:0 10px; font-size:16px; color:#000;}
	
	/* search_box */
	.search_box {position: relative; min-width:393px; max-width:100%; border-bottom:3px solid #000; padding:15px 0 7px 0px; margin-left:65px; margin-right:172px;}
	.search_box:before {content:""; display:block; clear:both;}
	.search_box > input {width:343px; font-size:22px; color:#333; border:0; padding:1px 0 0 0; -webkit-focus-ring-color: rgba(255, 255, 255, 0) !important;  outline: none !important;}
	.search_box .btn_sch {display:block; position:absolute; top:19px; right:-6px; font-size:0; width:50px; height:32px; margin-left:20px; line-height:0; text-indent: -999em; background:url('../images/icon_search.png') no-repeat 50% 0; background-size: 30px 32px;}
	/* util  */
	.util {position:absolute; top:10px; right:17px; padding: 0 0;}
	.util ul {width:100%; display:table;}
	.util ul li {position:relative; display:table-cell;}
	.util ul li > a {display:block;font-size:0; width:50px; height:32px; margin-left:6px; line-height:0; text-indent: -999em; }
	.util ul li > a.btn_my {background:url('../images/icon_my.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > a.btn_library {background:url('../images/icon_library.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > a.btn_zzim {background:url('../images/icon_zzim.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > a.btn_home {background:url('../images/icon_home.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > a.btn_sch {background:url('../images/icon_search.png') no-repeat 50% 0; background-size:30px 32px;}
.pagemain .header_top .util {position:absolute; top:19px; right:25px; padding: 0 0;}

