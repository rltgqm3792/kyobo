/* gnb  */
.gnb {height:56px; padding: 0 0; padding-left:36px;}
.gnb ul {width:100%; display:table;}
/* .gnb ul li {position:relative; width: calc(100% / 7); font-size: 20px; font-family: 'NotoB'; display:table-cell;} */
.gnb ul li {display:inline-block; font-size: 20px; font-family: 'NotoB'; padding-right:50px;}
.gnb ul li:first-child {padding-right:54px;}
.gnb ul li:nth-child(5) {padding-right:53px;}
.gnb ul li:last-child {padding-left:3px; padding-right:20px;}
.gnb ul li a {display:block; padding-top:19px; padding-bottom:8px; color: #000;}
.gnb ul li a.on {display:inline-block; background:url('../images/bg_gnb01.png') repeat-x 0 100%;}
.gnb ul li a.gnb_sam {width:46px; height:33px; background:url('../images/gnb_sam.png') no-repeat 0 4px; background-size:46px 14px; font-size:0; box-sizing: border-box;  text-indent: -999em;}
.gnb ul li a.gnb_sam.on {border-bottom:3px solid #000;}

/* subgnb box */
.subgnb_box {display: none; position:relative; width:100%;}
.dsblock {display: block;}
	/* subgnb01 */
	.subgnb01 {display: inline-block; margin-right:22px;}
	.subgnb01 ul {display:table;}
	.subgnb01 ul li {position:relative; padding-right:25px; font-size: 18px; font-family: 'NotoM'; display:table-cell;}
	.subgnb01 ul li a {display:inline-block; height: 32px; color: #000; padding-top:4px; border-bottom: 2px solid #fff; box-sizing: border-box;}
	.subgnb01 ul li a.on {border-bottom:2px solid #000; box-sizing: border-box;}
	.subgnb01 ul li:last-child {background:url('../images/bull_line01.png') no-repeat 100% 8px;}
	.subgnb01 ul.sam li {padding-right:10px; font-size: 16px;}
	.subgnb01 ul.sam {margin-right:-14px;}

	/* subgnb02 */
	.subgnb02 {display: inline-block; padding: 0 0 0 0; height:38px; vertical-align: top;}
	.subgnb02 ul {display:table;}
	.subgnb02 ul li {position:relative; font-size: 18px; text-align: center; font-family: 'NotoM'; display:table-cell;}
	.subgnb02 ul li a {display:inline-block; width: 66px; height: 32px; line-height: 32px; color: #000;}
	.subgnb02 ul li a.on {font-family: 'NotoR'; color:#fff; height: 32px; line-height: 32px; background-color: #333; border-radius:15px;}
/* filter01  */
.filter01 {position: absolute; top:-5px; right:-10px;}
/* title */
.title h2 {margin:21px 0 6px; font-size:20px;  font-family: 'NotoR'; letter-spacing: -1px;}

/* book_list01 */
.book_list01 {position: relative; width:100%; margin:7px auto 0;}
.book_list01 .list {position: absolute; left:0; top:0; display:flex; flex-flow:wrap; justify-content: space-between; margin-top:-18px; z-index: 10;}
.book_list01 .list li > a::before {content:""; display:block; clear:both;}
.book_list01 .list li {position:relative; flex: 0 0 auto; margin:20px 0 0;  width: calc(33.33% - 30px);}
.book_list01 .list li > a {display:block; position:relative; padding:0px 0 0; width:180px; height:auto;}
.book_list01 .list li .pic {position: relative; border:1px solid #777777; z-index:10;} 
.book_list01 .list li .pic > a > img {width:100%; height:260px;}
.book_list01 .list li .title {font-size:18px; color:#000; padding-top:8px;}
.book_list01 .list li .title .text {height:35px; text-align: left; line-height: 18px; display:-webkit-box; overflow:hidden; text-overflow:ellipsis;-webkit-line-clamp:2;-webkit-box-orient:vertical; font-family: 'NotoM';}
.book_list01 .list li .title .rank {display: inline-block; float:left; width:15%;min-height:35px; margin-top:-5px; font-size:20px; font-family: 'NotoBk';  letter-spacing: -1px;}
.book_list01 .list li .title .rank + .text {display: inline-block; float:left; width:85%; font-family: 'NotoM'; padding-top:0px; display:-webkit-box; overflow:hidden; text-overflow:ellipsis;-webkit-line-clamp:2;-webkit-box-orient:vertical;}
.book_list01 .list li .title .rank.rank100 {width:21%; font-size:21px !important;} 
.book_list01 .list li .title .rank.rank100 + .text {width:79%; padding-top:0px;} 
.book_list01 .data_no {font-size:24px; width:100%; height:600px; display: table; border:1px solid #777777; border:1px solid #777777;}
.book_list01 .data_no > span {display: inline-block; display:table-cell; vertical-align: middle; text-align: center;}

/* book view01     */
.book_view01 {position: relative; width:100%; margin:6px auto 0;}
.book_view01:after {display:block;clear:both;content:''}
.book_view01 .view li {position: relative;}
.book_view01 .view li .pic {display: inline-block; width:230px; height:340px; margin-right:28px; border:1px solid #777777; box-sizing: border-box;}
.book_view01 .view li .detail {position: absolute; top:0; left:258px; height: 340px;}
.book_view01 .view li .detail .title {font-size:36px; line-height: 38px; padding:9px 0 23px 0; border-bottom:1px solid #ccc;}
.book_view01 .view li .detail .info {height:150px; margin-top: 15px; z-index: 1000; border:0px solid #00f;}
.book_view01 .view li .detail .info > p {font-size:16px; font-family: 'NotoR'; padding:0 0 2px 0;}
.book_view01 .view li .detail .info > .text > span {position: relative; display: inline-block; padding:0 10px;}
.book_view01 .view li .detail .info > .text > span:first-child {padding:0;}
.book_view01 .view li .detail .info > .text > span:first-child:after {content:''}
.book_view01 .view li .detail .info > .text > span:after {position: absolute; top:4px; left:3px; font-size:11px; color:#999999; display:inline-block; clear:both; content:'|'}
.book_view01 .view li .detail .info > p.epub {color:#666;} 
.book_view01 .view li .detail .info > p.point {position: relative; padding:7px 0 0 0;}
.book_view01 .view li .detail .info > p.point .wrap {position: relative; top:1px; display:inline-block; width:92px; height:22px; background:url('../images/icon_point01.png') no-repeat 0 0; text-indent:-9999px; z-index:11;background-size:92px 44px;}
.book_view01 .view li .detail .info > p.point .wrap .value {position: absolute; top:0; left:0; display:inline-block; width:92px; height:22px; background:url('../images/icon_point01.png') no-repeat 0 -22px; text-indent:-9999px; z-index:11;background-size:92px 44px;}
.book_view01 .view li .detail .info > p.point .grade {display: inline-block; padding:0 16px 0 10px; font-size: 24px; font-family: 'NotoB';}
.book_view01 .view li .detail .info > p.point .unit {display: inline-block; font-size: 16px; font-family: 'NotoR'; vertical-align: top; line-height: 34px;}
.book_view01 .view li .detail .icon_state {min-width:62px; height:28px; padding:0 16px; border:1px solid #666666; border-radius: 15px; font-size:16px; line-height: 24px;}

/* book_view02 */
.book_view02 {position: relative; width:100%; margin:10px auto 21px;}
.book_view02 .view {display:flex; flex-flow:wrap; justify-content: space-between; margin-top:-20px;}
.book_view02 .view li > a::before {content:""; display:block; clear:both;}
.book_view02 .view li {position:relative; flex: 0 0 auto; margin:20px 0 0;  width: calc(20% - 19px);}
.book_view02 .view li > a {display:block; position:relative; padding:0px 0 0; width:110px; height:auto;}
.book_view02 .view li .pic {position: relative; border:1px solid #777777; box-sizing: border-box; z-index:10;} 
.book_view02 .view li .pic > a > img {width:100%; height:auto;}
.book_view02 .view li .title {font-size:16px; color:#000; font-family: 'NotoB'; margin-top:11px; height:20px; text-align: left; line-height: 18px; display:-webkit-box; overflow:hidden; text-overflow:ellipsis;-webkit-line-clamp:1;-webkit-box-orient:vertical;}
.book_view02 .view li .price {margin:4px 0 0 0; font-size:14px; color:#333; font-family: 'NotoR';}

/* tbl view01 */
.tbl_view01 {clear:both; margin:26px 0 0 0; border:1px solid #999999;border-radius:5px;overflow:hidden;}
.tbl_view01 table {width:100%;}
.tbl_view01 table th {padding:12px 8px; font-weight:normal; text-align:center; border-bottom:1px solid #999999; font-size:16px; font-family: 'NotoR'; background:#ededed;}
.tbl_view01 table td {padding:15px 8px; text-align:center; border-top:1px solid #ccc; font-size:16px; font-family: 'NotoR'; background:#fff;}
.tbl_view01 table td.tright {padding:10px 8px;text-align:right;}
.tbl_view01 table td.brline {border-right:1px solid #ccc;}
.tbl_view01 table tr:first-child th {border-top:0;border-radius:3px 0 0 0;}
.tbl_view01 table tr:first-child td {border-top:0;border-radius:0 3px 0 0;}
.tbl_view01 table tr:last-child th {border-radius:0 0 0 3px;}
.tbl_view01 table tr:last-child td {border-radius:0 0 3px 0;}
.tbl_view01 table td .price01 {display: inline-block; font-size:22px; font-family: 'NotoB'; line-height: 30px;}
.tbl_view01 table td .day01 {display: inline-block; font-size:16px; line-height: 30px;vertical-align:top; margin-top:-1px;}
.tbl_view01 table td .price02 {display: inline-block; font-size:16px; color:#666666; text-decoration: line-through;}
.tbl_view01 table td .icon_small01 {margin-top:5px;}
.tbl_view01 table td .icon_small01:first-child {margin-top:0;}

