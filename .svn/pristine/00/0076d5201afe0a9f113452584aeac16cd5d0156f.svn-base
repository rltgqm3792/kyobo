/* book_list01 */
.book_list01 {position: relative; display: block; width:100%; margin:7px auto 0;}
.book_list01 .list {position: relative; margin:-16px 0 0 -45px; z-index: 10;}
.book_list01 .list:after {display:block;clear:both;content:''}
.book_list01 .list li > a::before {content:""; display:block; clear:both;}
.book_list01 .list li {position:relative; float:left; width:33.33%; padding-left:45px; margin:18px 0 0; box-sizing: border-box;}
.book_list01 .list li > a {display:block; position:relative; padding:0px 0 0;}
.book_list01 .list li .pic {position: relative; border:1px solid #777777; z-index:10;}  
.book_list01 .list li .title {font-size:18px; color:#000; padding-top:10px;}
.book_list01 .list li .title .text {height:44px; text-align: left; line-height: 22px; display:-webkit-box; overflow:hidden; text-overflow:ellipsis;-webkit-line-clamp:2;-webkit-box-orient:vertical; font-family: 'NotoM';}
.book_list01 .list li .title .rank {display: inline-block; float:left; width:15%;min-height:35px; margin-top:-4px; font-size:20px; font-family: 'NotoBk';  letter-spacing: -1px;}
.book_list01 .list li .title .rank + .text {display: inline-block; float:left; width:85%; font-family: 'NotoM'; padding-top:0px; display:-webkit-box; overflow:hidden; text-overflow:ellipsis;-webkit-line-clamp:2;-webkit-box-orient:vertical;}
.book_list01 .list li .title .rank.rank100 {width:21%; font-size:21px !important;} 
.book_list01 .list li .title .rank.rank100 + .text {width:79%; padding-top:0px;} 
.book_list01 .data_no {font-size:24px; width:100%; height:600px; display: table;}
.book_list01 .data_no > span {display: inline-block; display:table-cell; vertical-align: middle; text-align: center;}
.book_list01 .list.sam li .title .text {padding-right:40px;}
.book_list01 .list li .pic .cover {position:relative; display: block; height:0;padding-top:146%;} /* 1 : 1.46 */
.book_list01 .list li .pic .cover img {position:absolute;left:0;top:0;width:100%;height:100%;}

