.pagination .num {display:inline-block; padding:4px 0px 0 0px; margin-top:0px;}
.pagination .num > a  {display:inline-block; min-width:30px; height:30px; line-height:27px; font-family:'NotoB'; color:#000; font-size:16px; text-align:center; margin:0 18px 0 0; border:0px solid #dedede; box-sizing: border-box;}
