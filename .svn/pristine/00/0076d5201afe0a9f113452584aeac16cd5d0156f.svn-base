.cover img:first-child {position: absolute;left: 0;top: 0;width: 100%; height: 100%;}*/

/** layout **/
.pagemain {overflow: hidden;}
.pagesub {overflow: hidden;}
#header_container {}
#header_container .header_fixed {position:fixed; top:0; width:100%; height:53px; padding:0; background:#fff; border-bottom:1px solid #999; z-index:1100;}
/* main */
.pagemain #header_container .header_fixed {height:112px !important;}

#body_container {width:100%; margin:53px auto 0px; padding:0 0 100px;  z-index:1000;}
#body_container:after {display:block;clear:both;content:''}

#body_container .content {width:100%; height:100%; padding:0 0 20px; box-sizing: border-box;}
#body_container .content_inner {width:100%; height:calc(100vh - 113px); padding:21px 28px 80px; overflow-y: auto; box-sizing: border-box;}
/* pageMain */
.pagemain #body_container {margin:112px auto 0px;}
.pagemain #body_container .content_inner {width:100%; height:calc(100vh - 164px); padding:16px 36px; overflow:hidden; box-sizing: border-box;}

/* content bottom  */
.content_bottom {position: relative; width:100%; height:60px; padding:0px 0 0 0; margin-left:0px; margin-right:0;  background:#000;z-index:1000;}
.content_bottom > .bottom_box {position: relative; width:100%; z-index:1100;}

/* pageMain */
.pagemain .content_bottom {position: relative; width:100%; height:53px; padding:0px 0 0 0; margin-left:0px; margin-right:0;  background:#fff; border-top:1px solid #cccccc; z-index:1000;}
.pagemain .content_bottom > .bottom_box {position: relative; width:100%; z-index:1100;}

#footer_container {position:fixed; bottom:0; width:100%; height:66px; padding:0px 0 0 0; background:#fcc; z-index:1000;}
#footer_container > .inner {position: absolute; top:0; left:0; margin:0 auto; color:#000; background:#00f; z-index:1100;}

/** content **/
.header_top {position: relative; padding:0 36px;}
	/* btn menu */
	.header_top a.btn_menu {position:absolute; top:10px; left:26px; width:52px; height:58px; display:block;font-size:0; line-height:0; background:url('../images/icon_menu.png') no-repeat 10px 10px; background-size:32px 28px; text-indent: -999em;}
	/* back */
	.header_top .btn_back {position:absolute; top:8px; left:14px; width:36px; height:28px; background:url('../images/icon_back.png') no-repeat 14px 2px; background-size:26px 32px;display:block;font-size:0; width:52px; height:38px; line-height:0; text-indent: -999em;}
	.header_top .top_area {position: absolute; top:16px; left:69px; color:#787878;}
	.header_top .top_area > .location > span {display: inline-block; padding:0 10px; font-size:16px; color:#000;}
	
	/* search_box */
	.search_box {position: relative; min-width:393px; max-width:100%; border-bottom:3px solid #000; padding:15px 0 7px 0px; margin-left:65px; margin-right:172px;}
	.search_box:before {content:""; display:block; clear:both;}
	.search_box > input {width:90%; font-size:22px; color:#333; border:0; padding:1px 0 0 0; margin-right:100px; -webkit-focus-ring-color: rgba(255, 255, 255, 0) !important;  outline: none !important;}
	.search_box .btn_sch {display:block; position:absolute; top:19px; right:-9px; font-size:0; width:50px; height:32px; margin-left:20px; line-height:0; text-indent: -999em; background:url('../images/icon_search.png') no-repeat 50% 0; background-size: 30px 32px;}
	/* util  */
	.util {position:absolute; top:10px; right:17px; padding: 0 0;}
	.util ul {width:100%; display:table;}
	.util ul li {position:relative; display:table-cell;}
	.util ul li  a, .util ul li button {display:block;font-size:0; width:50px; height:32px; margin-left:6px; line-height:0; text-indent: -999em; border:0}
	.util ul li > .btn_my {display:block;font-size:0; width:50px; height:32px; margin-left:6px; line-height:0; text-indent: -999em; background:url('../images/icon_my.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > .btn_library {background:url('../images/icon_library.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > .btn_zzim {background:url('../images/icon_zzim.png') no-repeat 50% 0; background-size:30px 32px; border:0;}
	.util ul li > .btn_home {background:url('../images/icon_home.png') no-repeat 50% 0; background-size:30px 32px;}
	.util ul li > .btn_sch {background:url('../images/icon_search.png') no-repeat 50% 0; background-size:30px 32px;}
.pagemain .header_top .util {position:absolute; top:19px; right:25px; padding: 0 0;}

/* gnb  */
.gnb {height:56px; padding: 0 0; padding-left:36px;}
.gnb ul {width:100%; display:table;}
.gnb ul li {display:inline-block; font-size: 20px; font-family: 'NotoB'; padding-right:50px;}
.gnb ul li:first-child {padding-right:54px;}
