@charset "utf-8";
/** Reset **/
html, body {height:100%;}
body {font-size:16x; line-height:1.4; font-family:'NotoM', ’Malgun Gothic’, Dotum, Arial, Sans-serif; color:#000; letter-spacing: -0.5px;}
table, th, td, input, select, button, textarea, pre {font:inherit; color:inherit;}
body, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, select, button, textarea, p, blockquote, table, th, td {margin:0; padding:0;}
h1, h2, h3, h4, h5, h6 {font-size:100%; font-weight: normal;}
ol, ul {list-style:none;}
img, fieldset {border:0;}
img {vertical-align:middle; }
a {color:#000; text-decoration:none;}
table {border-spacing:0; border-collapse:collapse; border:0;}
th, td {word-break:break-all; word-wrap:break-word;}
label {cursor:pointer;}
Input, button {border:0; border-radius:0;}
button {cursor: pointer; outline: none !important;}
textarea {resize:none;}
input, button, select, textarea {vertical-align:middle; font-family: 'NotoM';}
address, caption, cite, code, dfn, em, var {font-style:normal;}
hr {clear:both; display:none;}
blockquote, q {quotes:none;}
abbr, acronym {border:0;}
legend, caption {visibility:hidden; overflow:hidden; line-height:0; width:0; height:0; margin:0; padding:0; text-indent:-99999px; font-size:0;}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section, main {display:block;}

.hidden {position:absolute; visibility:hidden; overflow:hidden; height:0; line-height:0; width:0; margin:0; padding:0; font:0/0 Arial;}

/* form */
input {font-size:15px; border:1px solid #d7d7d7; height:32px; padding:3px 8px 2px 8px; background:#fff; vertical-align:top; border-radius: 0px;  -webkit-appearance: none;  -moz-appearance: none; appearance: none;}

/* Medium */
@font-face {
	font-family: 'NotoM';
	font-style: normal;
	font-weight: 500;
	src: local('Noto Sans Medium'), local('NotoSans-Medium');
	src: /* url(/fonts/NotoSans-Medium.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Medium.woff2) format('woff2'),
	url(../font/NotoSansKR-Medium.woff) format('woff');
	}
/* Bold */
@font-face {
	font-family: 'NotoB';
	font-style: normal;
	font-weight: 700;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Bold.woff2) format('woff2'),
	url(../font/NotoSansKR-Bold.woff) format('woff');
}
/* Regular */
@font-face {
	font-family: 'NotoR';
	font-style: normal;
	font-weight: 300;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Regular.woff2) format('woff2'),
	url(../font/NotoSansKR-Regular.woff) format('woff');
}
/* Black */
@font-face {
	font-family: 'NotoBk';
	font-style: normal;
	font-weight: 700;
	src: local('Noto Sans Bold'), local('NotoSans-Bold');
	src: /* url(/fonts/NotoSans-Bold.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Black.woff2) format('woff2'),
	url(../font/NotoSansKR-Black.woff) format('woff');
}

/* Light */
@font-face {
	font-family: 'NotoL';
	font-style: normal;
	font-weight: 300;
	src: local('Noto Sans Light'), local('NotoSans-Light');
	src: /* url(/fonts/NotoSans-Light.eot?#iefix) format('embedded-opentype'), */
	url(../font/NotoSansKR-Light.woff2) format('woff2'),
	url(../font/NotoSansKR-Light.woff) format('woff');
}

/* form */
.check_box, .radio_box {display:inline-block; margin-right:10px;}
.check_box input[type='checkbox'], .radio_box input[type='radio']{position:absolute;width:26px;height:26px;margin:0;opacity:0;}
.check_box label, .radio_box label{display:inline-block;min-height:26px;color:#333;font-size:15px;line-height:26px;vertical-align:top;}
.check_box label:before, .radio_box label:before{display:inline-block;width:26px;height:26px;margin-right:14px;background:url('../images/icon_checkbox.png') no-repeat 0 0;background-size:26px 26px;vertical-align:top;content:'';}
.check_box label:before{background:url('../images/icon_checkbox_on.png') no-repeat 0 0;background-size:26px 26px;}
.check_box input[type='checkbox']:checked + label:before{background:url('../images/icon_checkbox_on.png') no-repeat 0 0;background-size:26px 26px;}
.radio_box input[type='radio']:checked + label:before{background:url('../images/icon_checkbox_on.png') no-repeat 0 0;background-size:26px 26px;}
.check_box input[type='checkbox']:disabled + label, .radio_box input[type='radio']:disabled + label{opacity:0.4;}
.radio{width:100%;padding:10px 0px 0 0;}
.radio .radio_box{float:left;}
.radio:after{display:block;clear:both;content:"";}

select { height:32px; font-size:18px; font-family: 'NotoM'; padding:3px 5px 3px 5px; vertical-align:top; margin-right:6px; /*background:#fff url('../images/icon_select01.png') no-repeat right center;*/ background-size:auto 8px;	border:0px; border-radius: 0px;  -webkit-appearance: none;  -moz-appearance: none; appearance: none; outline:none !important; box-sizing: border-box;}
select::-ms-expand { display: none;}
.selectbox.w01 select {width: 148px !important;}
.selectbox.w02 select {width: 268px !important;}

.selectbox.w01 .select {width: 148px !important;}
.selectbox.w02 .select {width: 268px !important;}
.select-hidden {display: none; visibility: hidden; padding-right: 10px;}
.select {cursor: pointer; display: inline-block; position: relative; font-size: 16px; color: #333; letter-spacing:-0.5px; width: 168px; height: 40px;}
.select-styled {position: absolute; top: 0; right: 0; bottom: 0; left: 0; padding: 8px 15px; -moz-transition: all 0.2s ease-in; -o-transition: all 0.2s ease-in; -webkit-transition: all 0.2s ease-in; transition: all 0.2s ease-in;}
.select-styled:after {content: ""; width: 15px; height: 10px; background:url('../images/icon_select.png') no-repeat right center;  background-size: 15px 9px; position: absolute;	top: 16px; right: 10px;}
.select-styled:hover {background-color: #fff;}
.select-styled:active, .select-styled.active {background-color: #fff;}
.select-styled:active:after, .select-styled.active:after {top: 15px; background:url('../images/icon_select_on.png') no-repeat right center;}
.select-options {display: none; position: absolute; top: 100%; right: 0; left: 0; z-index: 999; margin: 0; padding: 0; list-style: none; background-color: #fff; border: 1px solid #333;}
.select-options li {margin: 0; padding:6px 0 5px; text-indent: 15px; border-top: 0px solid #333; -moz-transition: all 0.15s ease-in; -o-transition: all 0.15s ease-in;
	-webkit-transition: all 0.15s ease-in; transition: all 0.15s ease-in;}
.select-options li:hover {color: #c0392b; background: #fff;}
.select-options li[rel="hide"] {display: none;}   
.disabled {cursor: default; border:0; opacity: .4;}

.dsblock {display: block;}
.dsnone {display: none !important;}

