.book_view01 .view li .detail .detail_inner .btn_area {}
.book_view01 .view li .detail .detail_inner .info > p.epub {color:#666; position: relative;} 
.book_view01 .view li .detail .detail_inner .info > p.epub > span {position: relative; display: inline-block; padding:0 15px;}
.book_view01 .view li .detail .detail_inner .info > p.epub > span:first-child {padding:0;}
.book_view01 .view li .detail .detail_inner .info > p.epub > span:first-child:after {content:''}
.book_view01 .view li .detail .detail_inner .info > p.epub > span:after {content:'/';position: absolute; top:2px; left:3px; font-size:15px; color:#999999; display:inline-block; clear:both;}

.book_view01 .view li .detail .detail_inner .info > p.isbn {color:#666;} 
.book_view01 .view li .detail .detail_inner .info > p.point {position: relative; padding:7px 0 0 0;}
.book_view01 .view li .detail .detail_inner .info > p.point .wrap {position: relative; top:1px; display:inline-block; width:92px; height:22px; background:url('../images/icon_point01.png') no-repeat 0 0; text-indent:-9999px; z-index:11;background-size:92px 44px;}
.book_view01 .view li .detail .detail_inner .info > p.point .wrap .value {position: absolute; top:0; left:0; display:inline-block; width:92px; height:22px; background:url('../images/icon_point01.png') no-repeat 0 -22px; text-indent:-9999px; z-index:11;background-size:92px 44px;}
.book_view01 .view li .detail .detail_inner .info > p.point .grade {display: inline-block; padding:0 16px 0 10px; font-size: 24px; font-family: 'NotoB';}
.book_view01 .view li .detail .detail_inner .info > p.point .unit {display: inline-block; font-size: 16px; font-family: 'NotoR'; vertical-align: top; line-height: 34px;}
.book_view01 .view li .detail .detail_inner .icon_state01 {min-width:62px; height:28px; padding:0 16px; margin-right:5px; border:1px solid #666666; border-radius: 15px; font-size:16px; line-height: 24px;}
.book_view01 .view li .detail .detail_inner .icon_state02 {min-width:62px; height:28px; padding:0 2px; margin-right:-3px; font-size:16px; line-height: 24px;}

/* book_view02 */
.book_view02 {position: relative; width:100%; margin:0px auto 21px;}
.book_view02 .view {position: relative; margin:-20px 0 20px -24px;}
.book_view02 .view:after {display:block;clear:both;content:''}
