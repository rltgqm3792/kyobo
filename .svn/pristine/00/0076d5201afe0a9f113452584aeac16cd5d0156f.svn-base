.gnb ul li a.gnb_sam {width:46px; height:56px; font-size:0; position: relative; top:-24px; box-sizing: border-box; text-indent: -999em;}
.gnb ul li a.gnb_sam:after {content: ""; width: 46px; height:56px; position: absolute;	top: 0px; left: 0px; background:url('../images/gnb_sam.png') no-repeat 0 28px; background-size:46px 14px;}
.gnb ul li a.gnb_sam.on {display:inline-block; background:url('../images/bg_gnb01.png') repeat-x 0 100%;}

/* subgnb box */
.subgnb_box {display: none; position:relative; width:100%;}
.subgnb_box:first-child {display: block;} 
.subgnb_box .btn_back01 {display: inline-block; padding:3px 0 7px 21px; font-size:18px; box-sizing: border-box;}
.subgnb_box .btn_back01:before{display:inline-block; position: absolute; left:0; top:5px; width:10px;height:20px; background:url('../images/icon_back01.png') no-repeat 0px 0;background-size:10px 20px;vertical-align:top;content:'';}

	/* subgnb01 */
	.subgnb01 {display: inline-block; margin-right:12px; }
