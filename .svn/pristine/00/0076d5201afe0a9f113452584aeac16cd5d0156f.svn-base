/* book view01     */
.book_view01 {position: relative; width:100%; margin:6px auto 0; box-sizing: border-box;}
.book_view01:after {display:block;clear:both;content:''}
.book_view01 .view li {position: relative;}
.book_view01 .view li .pic {display: inline-block; float:left; position: relative; width:230px; height:340px; margin-right:28px; border:1px solid #777777; box-sizing: border-box;}
.book_view01 .view li .pic > img {width:100%; height:100%;}
.book_view01 .view li .detail {display: inline-block; position: relative; float:left; width: calc(100% - 258px); margin-right:-250px; border:0px solid #00f;}
.book_view01 .view li .detail .detail_inner {overflow: visible; min-height:272px; max-height: auto; margin:0 0 20px;}
.book_view01 .view li .detail .detail_inner > .title {border-bottom:1px solid #ccc; padding:9px 0 0;}
.book_view01 .view li .detail .detail_inner > .title > h2 {font-size:36px; line-height:38px; padding:0px 0 14px 0;}
.book_view01 .view li .detail .detail_inner > .title > h2.fsize01 {height:64px; font-size:28px; line-height:30px; padding:0px 0 4px 0;}
.book_view01 .view li .detail .detail_inner > .title > .sub {display: inline-block; font-size:20px; line-height:24px; padding-bottom:10px; margin-top:-4px;}
